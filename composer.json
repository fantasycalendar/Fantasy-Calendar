{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.2",
        "ext-json": "*",
        "barryvdh/laravel-ide-helper": "^2.6",
        "code16/sharp": "^4.2",
        "fideloper/proxy": "^4.0",
        "graham-campbell/markdown": "^11.0",
        "guzzlehttp/guzzle": "^6.5",
        "laravel/framework": "^6.0",
        "laravel/tinker": "^1.0",
        "league/fractal": "^0.18.0",
        "mews/purifier": "^3.1",
        "nesbot/carbon": "^2.22"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.2",
        "facade/ignition": "^1.4",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^3.0",
        "phpunit/phpunit": "^8.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "pre-install-cmd": [
            "@php -r \"file_exists('storage/framework/cache') || mkdir('storage/framework/cache');\"",
            "@php -r \"file_exists('storage/framework/sessions') || mkdir('storage/framework/sessions');\"",
            "@php -r \"file_exists('storage/framework/views') || mkdir('storage/framework/views');\""
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan vendor:publish --provider='Code16\\Sharp\\SharpServiceProvider' --tag=assets --force",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
